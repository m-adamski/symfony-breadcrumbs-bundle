{
    "name": "m-adamski/symfony-breadcrumbs-bundle",
    "description": "The breadcrumbs tool simplifies the generation and display process of breadcrumbs",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Maciej Adamski",
            "email": "m.adamski@ideacode.pl"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/twig-bundle": "^7.0"
    },
    "require-dev": {
        "kint-php/kint": "^5.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Adamski\\Symfony\\BreadcrumbsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adamski\\Symfony\\BreadcrumbsBundleTests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
